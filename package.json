{
  "name": "kenshi",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "NODE_OPTIONS=--max_old_space_size=16384 npm run docs:build && npm run blog:build && vite build",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "docs:watch": "chokidar \"src/routes/docs/**/*.cadey\" -c \"node docs.js {path}\"",
    "docs:build": "node docs.js",
    "blog:watch": "chokidar \"src/routes/blog/**/*.cadey\" -c \"node blog.js\"",
    "blog:build": "node blog.js",
    "unchained:install:cp": "for file in ../unchained/install/*; do filename=$(basename $file .sh); cp $file static/unchained/$filename; done;",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@carbon/charts-svelte": "^1.13.4",
    "@zerodevx/svelte-toast": "^0.6.3",
    "chokidar-cli": "^3.0.0",
    "cli-progress": "^3.11.2",
    "d3": "^7.8.5",
    "d3-cloud": "^1.2.7",
    "d3-sankey": "^0.12.3",
    "d3-sankey-circular": "^0.34.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte": "^2.33.2",
    "jsdom": "^20.0.1",
    "layercake": "^6.1.0",
    "ora": "^6.1.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.8.8",
    "prisma": "^5.9.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "svelte": "^4.2.1",
    "svelte-loading-spinners": "^0.1.7"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.53.0",
    "@kenshi.io/solidquery": "^0.2.0",
    "@mongodb-js/zstd": "^1.2.0",
    "@prisma/client": "^5.9.0",
    "@sendgrid/client": "^8.1.3",
    "@sendgrid/mail": "^7.7.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.25.1",
    "@threlte/core": "^7.3.0",
    "@threlte/extras": "^8.11.2",
    "@web3-onboard/core": "^2.20.4",
    "@web3-onboard/injected-wallets": "^2.10.2",
    "@web3-onboard/uauth": "^2.1.1",
    "@web3-onboard/walletconnect": "^2.4.1",
    "base-ex": "^0.8.1",
    "buffer": "^6.0.3",
    "cadey": "^1.1.0",
    "carbon-components-svelte": "^0.80.0",
    "carbon-icons-svelte": "^12.10.0",
    "carbon-pictograms-svelte": "^12.8.0",
    "chart.js": "^4.4.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "clipboard-copy": "^4.0.1",
    "d3-shape": "^3.1.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.1",
    "format-thousands": "^2.0.0",
    "intl-messageformat": "^10.5.3",
    "js-base64": "^3.7.5",
    "js-sha3": "^0.9.3",
    "mermaid": "^10.9.0",
    "monaco-editor": "^0.38.0",
    "monaco-themes": "^0.4.4",
    "mongodb": "^6.3.0",
    "percentile": "^1.6.0",
    "prettier-plugin-solidity": "^1.1.3",
    "pygments": "^0.2.1",
    "rxjs": "^7.5.7",
    "shave": "^5.0.2",
    "sializer": "^2.0.1",
    "svelte-inview": "^4.0.1",
    "svelte-skeleton": "^1.3.1",
    "three": "^0.164.1",
    "uuidv7": "^1.0.1",
    "vite": "^4.4.2",
    "yaml": "^2.3.0"
  }
}
